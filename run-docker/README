docker network create fgnet
docker network ls

docker run --name fgdb --network fgnet --rm --detach eosc/fgdb
docker run --name apiserverdaemon --network fgnet --rm --detach eosc/apiserverdaemon socat TCP-LISTEN:3306,fork TCP:fgdb:3306
docker run --name fgapiserver --network fgnet --rm --detach --publish 8080:8888 eosc/fgapiserver-no-auth socat TCP-LISTEN:3306,fork TCP:fgdb:3306

docker exec apiserverdaemon /usr/share/tomcat8/bin/catalina.sh run
docker exec fgapiserver /usr/bin/python fgapiserver.py

mvn test -P IntegrationTests -Dfuturegateway.uri=http://localhost:8080/
